<!-- Mostra il modulo del campo magnetico misurato dallo smartphone.
Il valore reale viene mediato attarverso il valore statistico.
-->
<phyphox version="1.7" locale="it">

	<title>Misura statistica del modulo del campo magnetico</title>                   <!-- titolo dello strumento -->

	<category>Campo Mg statistico</category>                 <!-- insieme di strumenti -->

	<description>                                                   <!-- descrizione dell'esperimento-->
        Controlla la distribuzione statistica dei dati del sensore.
        Questo esperimento mostra semplicemente i dati grezzi dal sensore in un istogramma, che dovrebbe formare una distribuzione gaussiana quando il dispositivo Ã¨ a riposo. 
	A seconda del rumore del sensore, potresti voler modificare la dimensione del binning. Il pulsante Cancella dati resetta la misura e memorizza il valore di campo precedente.
  </description>
	
	<icon format="base64">        
	</icon>
	
	<color>yellow</color>  <!-- colore icona -->
	
	    <data-containers>
					<!-- contenitori per i valori dallo smartphone  -->
	    		<container size="0" static="false" init="">mfX</container> <!-- contenitore per il campo magnetico lungo l'asse x dello smartphone -->
	    		<container size="0" static="false" init="">t</container> <!-- contenitore per il tempo -->
<!-- contenitori per l'indagine statistica  -->   
          <container size="1">binSize</container>                     <!-- dimensione dell'intervallo -->
	        <container size="0">binStart</container>                    <!-- intervallo di partenza -->
	        <container size="0">binCount</container>                    <!-- numero di intervalli -->
	        <container size="1">average</container>                     <!-- valor medio di mf_x -->
		<container size="1">averageP</container>                     <!-- valor medio PRIMA DEL RESET -->
	        <container size="1">deviation</container>                   <!-- deviazione standard di mf_x -->
	        <container size="1">count</container>                       <!-- numero di conteggi di mf_x -->
<!-- contenitori per il calcolo della curva gaussiana teorica -->
          <container size="1">minX</container>                        <!-- valore minimo misurato di mf_x -->
          <container size="1">maxX</container>                        <!-- valore massimo di mf_x -->
          <container size="500">gaussianX</container>                 <!-- valore mf_x per la gaussiana -->
          <container size="500">gaussian</container>                  <!-- il valore y della gaussiana -->
     </data-containers>
		 
<input>    
        <sensor type="magnetic_field" rate="0">    <!-- sensor= sensore dello smartphone  type= accelerometro rate=0 alla massima velocitÃ  possibile-->
            <output component="abs">mfX</output>   <!-- prendi i valori della componente x e mettili in mfX-->
            <output component="t">t</output>      <!-- prendi i valori del tempo e mettili in t -->
        </sensor>
</input>

<output>
</output>

<analysis sleep="0" onUserInput="false">
        <binning>
            <input clear="false">mfX</input>
            <input as="dx" clear="false">binSize</input>
            <output clear="true">binStart</output>   <!-- calcolo dell'intervallo iniziale -->
	          <output clear="true">binCount</output>   <!-- calcolo del numero di intervalli --> 
        </binning>

        <average>
            <input clear="false">mfX</input>        
            <output as="average">average</output>     <!-- calcolo del valor medio -->
            <output as="stddev">deviation</output>    <!-- calcolo della dev. st. -->
        </average>

        <count>
            <input clear="false">mfX</input>
            <output as="count">count</output>          <!-- numero di valori di mf_x -->
        </count>

        <subrange>                                     <!-- firt element of binStart -->
            <input clear="false">binStart</input>       
            <input as="from" type="value">0</input>
            <input as="length" type="value">1</input>
            <output>minX</output>                      <!-- calcolo del valor minimo -->
        </subrange>

        <max> 
            <input as="y" clear="false">binStart</input>
            <output as="max">maxX</output>              <!-- calcolo del valore massimo -->
        </max>
	
        <ramp>
            <input as="start" clear="false">minX</input>
            <input as="stop" clear="false">maxX</input>   <!-- calcolo dei valori x della gaussiana -->
            <output>gaussianX</output>
        </ramp>

        <formula formula="[4]*[5]*exp(-0.5*([1_]-[2])^2/[3]^2)/(2.506628*[3])">
            <input clear="false">gaussianX</input>
            <input clear="false">average</input>
            <input clear="false">deviation</input>
            <input clear="false">binSize</input>
            <input clear="false">count</input>
            <output>gaussian</output>                 <!-- calcolo dei valori y della gaussiana -->
        </formula>

  </analysis>
	
  <views>   <!-- Blocco schermate -->  
    <view label="Misura statistica di mf">    <!-- grafico temporale di mf_x in funzione del tempo -->
            <graph label="misura di mf(t)" aspectRatio="2.5" style="lines" lineWidth="1" partialUpdate="true" history="1" labelX="t" labelY="mf" labelZ="" unitX="s" unitY="ÂµT" unitZ="" logX="false" xPrecision="3" yPrecision="4" zPrecision="3" scaleMinX="auto" scaleMaxX="auto" scaleMinY="auto" scaleMaxY="auto" scaleMinZ="auto" scaleMaxZ="auto" minX="0" maxX="0" minY="0" maxY="0" minZ="0" maxZ="0"  mapWidth="0" >
                <input axis="x">t</input>
                <input axis="y">mfX</input>
            </graph>

            <graph label="Histogram" labelX="mf" labelY="Count" partialUpdate="false" unitX="ÂµT" xPrecision="4"> <!-- istogramma dei valori di mf_x + gaussiana -->
                <input axis="x">gaussianX</input>
                <input axis="y" style="lines" lineWidth="2">gaussian</input>
                <input axis="x">binStart</input>
                <input axis="y" style="vbars" color="E0E040" lineWidth="0.9">binCount</input>
            </graph>

            <edit label="Bin size" unit="ÂµT" default="0.01" signed="false">   <!-- spazio per l'inserimento dell'intervallo iniziale -->              
                <output>binSize</output>
            </edit>
            
						<value label="mf" size="1" precision="3" unit="ÂµT">
                <input>mfX</input>                                           <!-- valore temporale di mf_x -->
            </value>

            <value label="valor medio mf" precision="3" scientific="false" unit="ÂµT" factor="1">
                <input>average</input>                                        <!-- valor medio di mf_x -->
            </value>
            <value label="Standard deviation" precision="3" scientific="false" unit="ÂµT" factor="1">
                <input>deviation</input>                                       <!-- deviazione standard di mf_x -->   
            </value>
            <value label="Conteggi" precision="0" scientific="false" unit="" factor="1">
                <input>count</input>                                           <!-- numero di conteggi --> 
            </value>
      <button label="Cancella Dati">
           <input>average</input>
	   <output>averageP</output>
	   <input type="empty" />
           <output>mfX</output>
	   <input type="empty" />
	   <output>t</output>
	   <input type="empty" />
	   <output>binCount</output>
	   <input type="empty" />
	   <output>average</output>
	   <input type="empty" />
	   <output>deviation</output>
	   <input type="empty" />
	   <output>count</output>
           <input type="empty" />
	   <output>minX</output>
	   <input type="empty" />
	   <output>maxX</output>
	   <input type="empty" />
	   <output>gaussianX</output>
	   <input type="empty" />
	   <output>gaussian</output>
      </button>
      <value label="Valore di Campo terrestre" precision="3" scientific="false" unit="ÂµT" factor="1">
                <input>averageP</input>
      </value>
    </view>
  </views>
	
	<export>
    <set name="Dati grezzi">
      <data name="tempo (s)">t</data>
      <data name="mf (ÂµT)">mfX</data>
    </set>

        <set name="Dati statistici">
	<data name="valor medio mf (ÂµT)">average</data>
	<data name="dev. st. mf (ÂµT)">deviation</data>
        <data name="Bin start (ÂµT)">binStart</data>
        <data name="Bin count">binCount</data>
        </set>
  </export>
	
</phyphox>
